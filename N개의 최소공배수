#include <stdio.h>
#include <stdbool.h>
#include <stdlib.h>
// 최대공약수, 유클리드 호제법 이용   
int GCD (int a, int b){
    int temp = 0;
    // 큰 수를 a로 배치
    if (a<b){
        temp = b;
        b = a;
        a = temp;
    }
    // 유클리드 호제법
    while(b!=0){
        temp = a % b;
        a = b;
        b = temp;
    }
    return a;
}
// 최소공배수 = a*b/gcd
int LCD (int a, int b, int gcd){
    return a*b/GCD(a, b);
}

// arr_len은 배열 arr의 길이입니다.
int solution(int arr[], size_t arr_len) {
    int answer = 1;
    for(auto& a:arr){
        answer = LCD(answer, a[i]);
    }
    return answer;
}
